{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "GamingLibrary": "Information"
      }
    }
  },
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "GamingLibrary",
    "ConnectionTimeoutSeconds": 30,
    "EnableDetailedErrors": false
  },
  "AllowedHosts": "*"
}